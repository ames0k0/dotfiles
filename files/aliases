alias s='rm -R'
alias S='sudo rm -R'
alias ll='ls -l'
alias ins='sudo apt-get install'

alias kv='vim ~/.vimrc'
alias kz='vim ~/.zshrc'
alias kb='vim ~/.bashrc'
alias kt='vim ~/.tmux.conf'
alias kza='vim ~/.zsh_aliases'
alias kba='vim ~/.bash_aliases'

alias upd='apt-get update && apt-get upgrade -y'
alias upg='apt-get dist-upgrade'
